{
  "name": "@mxydl2009/promise-extra",
  "version": "2.1.1",
  "description": "provide promise extra functions",
  "module": "dist/dist.mjs",
  "main": "dist/dist.cjs",
  "scripts": {
    "docs": "jsdoc -c jsdocConfig.json",
    "live": "live-server docs --watch docs",
    "preview": "npm run docs && npm run live",
    "watch": "nodemon --watch ./src --exec \"npm run docs\" & npm run live",
    "build:esm": "rollup -c rollup.config.esm.mjs",
    "build:cjs": "rollup -c rollup.config.mjs",
    "build": "npm run build:esm && npm run build:cjs",
    "docs:md": "jsdoc2md src/*.js > docs.md",
    "dev": "vite",
    "test": "mocha test/*.test.js --timeout 5000",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "promise",
    "extra",
    "limited",
    "concurrent",
    "last"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.22.3",
    "@babel/runtime-corejs3": "^7.22.3",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-plugin-istanbul": "^7.0.0",
    "chai": "^4.3.7",
    "jsdoc": "^4.0.3",
    "jsdoc-to-markdown": "^8.0.1",
    "live-server": "^1.2.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.1.4",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "vite": "^5.3.3"
  }
}
