{
  "name": "@mxydl2009/promise-extra",
  "version": "1.0.0",
  "description": "provide promise extra functions",
  "module": "dist/dist.mjs",
  "main": "dist/dist.js",
  "scripts": {
    "docs": "jsdoc -c jsdocConfig.json",
    "live": "live-server docs --watch docs",
    "preview": "npm run docs && npm run live",
    "watch": "nodemon --watch ./src --exec \"npm run docs\" & npm run live",
    "build": "rollup -c rollup.config.mjs",
    "docs:md": "jsdoc2md src/*.js > README.md",
    "dev": "vite",
    "test": "mocha test/*.test.js"
  },
  "keywords": [
    "promise",
    "extra",
    "limited",
    "concurrent"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "jsdoc": "^4.0.3",
    "jsdoc-to-markdown": "^8.0.1",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.4",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "vite": "^5.3.3",
    "chai": "^4.3.7",
    "mocha": "^10.2.0"
  }
}
